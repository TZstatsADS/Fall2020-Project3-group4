   "source": [
    "from sklearn.naive_bayes import GaussianNB\n",
    "import time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_naive_bayes(feature_train,label_train):\n",
    "    start = time.time()\n",
    "    gnb = GaussianNB().fit(feature_train,label_train)\n",
    "    end = time.time()\n",
    "    train_time = end-start\n",
    "    return [train_time,gnb]"
   ]
  }
